@model IEnumerable<Model.Framework.ViewKQ>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<h2>Tổng hợp</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IDQN)
